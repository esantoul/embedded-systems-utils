  "cmake.configureOnOpen": false,
  "CppHelper.HeaderGuardPattern": "{FILE}_HPP",
  "editor.formatOnSave": true,
  "editor.detectIndentation": false,
  "editor.tabSize": 2,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "C_Cpp.formatting": "vcFormat",
  "C_Cpp.vcFormat.indent.gotoLabels": "leftmostColumn",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.block": "newLine",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.function": "newLine",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.lambda": "sameLine",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.namespace": "newLine",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.type": "newLine",
  "C_Cpp.vcFormat.space.pointerReferenceAlignment": "right",
  "C_Cpp.vcFormat.space.preserveInInitializerList": false,
  "C_Cpp.vcFormat.space.withinInitializerListBraces": false,
  "C_Cpp.vcFormat.newLine.closeBraceSameLine.emptyFunction": true,
  "clangd.arguments": [
    "--background-index",
    "--query-driver=**/arm-none-eabi-*",
    "--compile-commands-dir=${workspaceRoot}/build"
  ],
  // "editor.defaultFormatter": "xaver.clang-format",
  "clang-format.style": "InheritParentConfig",
  "cmake.configureOnEdit": false,
  "cmake.configureSettings": {
    "EMBODME_COMMON_WITH_TESTS": "ON", // this is how you select options when you configure with vscode
    "EMBODME_COMMON_WITH_EXAMPLES": "ON"
  }
